"*":
  Zen:
    gutter: true
    minimap: true
  "activate-power-mode":
    autoToggle: false
  core:
    disabledPackages: [
      "welcome"
      "linter-scss-lint"
    ]
    ignoredNames: [
      ".DS_Store"
      "Thumbs.db"
      ".localized"
    ]
    themes: [
      "pristine-ui"
      "one-dark-syntax"
    ]
  editor:
    autoIndentOnPaste: false
    invisibles: {}
    scrollPastEnd: true
    showIndentGuide: true
    softWrap: false
    softWrapAtPreferredLineLength: true
    useShadowDOM: true
  "exception-reporting":
    userId: "9c627fef-ee29-c034-821b-fed2efd80fe4"
  "fuzzy-finder":
    ignoredNames: [
      ".git"
      "node_modules"
      "node"
      "build"
    ]
  "isotope-ui": {}
  linter: {}
  "linter-jshint":
    disableWhenNoJshintrcFileInPath: true
    esversion: 6
    lintJSXFiles: true
  "linter-rust":
    executablePath: "/usr/local/bin/rustc"
  "linter-scss-lint":
    executablePath: "/usr/local/bin/scss-lint"
  "markdown-preview":
    breakOnSingleNewline: true
    liveUpdate: false
  metrics:
    userId: "646e00c1c0a9df20621b06ed7f7251374c38fce8"
  minimap:
    absoluteMode: true
    plugins:
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
  "one-dark-ui": {}
  racer: {}
  script:
    enableExecTime: false
    stopOnRerun: true
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
  whitespace:
    ensureSingleTrailingNewline: false
".text.todotxt":
  editor:
    autoIndent: true
    preferredLineLength: 120
"source.gfm":
  editor:
    preferredLineLength: 120
    softWrap: true
